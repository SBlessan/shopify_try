

<h1>Date Page Static Heading</h1>
<p>Date: <input type="date" id="datepicker" onClick = function()></p>

{% schema %}
  {
    "name": "Section name",
    "settings": [
      {
        "type":"text",
        "id":"heading",
        "label":"Heading",
        "default":"Heading"
      }
    ],
    "presets":[{
      "category":"date",
      "name":"Date Custom"
    }]
    
  }
{% endschema %}

<script>
  window.onload = function() {
    if(window.jQuery){
      let $ = window.jQuery
      $( function() {
    $( "#datepicker" ).datepicker({
        minDate: +1,
        maxDate: '2M'
    });
  } );
    }
  }
  

  $ function()
  
</script>


